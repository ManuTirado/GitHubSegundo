<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="_07_Ejercicios.MainPage"
             Title="MainPage">
    <VerticalStackLayout>
        <HorizontalStackLayout>
            <Label
                Text="Nombre: "/>
            <Entry
            x:Name="entry1"/>
        </HorizontalStackLayout>
        <HorizontalStackLayout>
            <Label
                Text="Apellidos: "/>
            <Entry
            x:Name="entry2"/>
        </HorizontalStackLayout>

        <Button 
            x:Name="btnMainPage"
            Text="Navegar a Pagina Tabbed"
            Clicked="OnBotonMainPage"/>
        <Button
                x:Name="btnPagina4"
                Text="Página 4"
                Clicked="onBotonPagina4"/>
        <Button
                x:Name="btnPagina5"
                Text="Página 5"
                Clicked="onBotonPagina5"/>
    </VerticalStackLayout>
</ContentPage>