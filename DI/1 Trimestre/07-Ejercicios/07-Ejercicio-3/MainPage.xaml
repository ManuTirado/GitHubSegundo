<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:local="clr-namespace:_07_Ejercicio_3.Pages"
             x:Class="_07_Ejercicio_3.MainPage"
            FlyoutLayoutBehavior="SplitOnLandscape">

    <FlyoutPage.Flyout>
        <local:PaginaFlyout x:Name="PaginaFlyout" />
    </FlyoutPage.Flyout>

    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <local:PaginaContactos />
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>
</FlyoutPage>
